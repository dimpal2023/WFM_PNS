@model IEnumerable<Wfm.App.Core.Model.SubDepartmentMasterMetaData>
@{
    int i = 0;
}
<table id="tableExport" class="display table table-hover table-checkable order-column m-t-20 width-per-100">
    <thead>
        <tr>
            <th>#</th>
            <th> Sub Department </th>
            <th>Created By</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            i = i + 1;
            <tr>
                <td>@i</td>
                <td>@item.SUBDEPT_NAME</td>
                <td>@item.CREATED_BY</td>
                <td>
                    <a href="javascript:void(0)" onclick="editSubDepartment('@item.SUBDEPT_ID')">
                        <i class="fas fa-edit"></i>
                    </a>
                </td>
            </tr>
        }

    </tbody>
</table>
<script>
    $("document").ready(function () {
        $('#tableExport').DataTable({
            dom: 'Bfrtip',
            buttons: [
                'copy',
                'csv',
                'excel',
                'pdf',
                'print'
            ]});
    });
</script>


